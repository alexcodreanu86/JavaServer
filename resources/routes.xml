        <path>/logs</path>
        <method>GET</method>
        <auth>true</auth>
    </route>
    <route>
        <path>/method_options</path>
        <method>GET</method>
        <method>HEAD</method>
        <method>POST</method>
        <method>OPTIONS</method>
        <method>PUT</method>
